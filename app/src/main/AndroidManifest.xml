<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cisco.photogame" >

    <uses-sdk
            android:minSdkVersion="16"
            android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/circle_small_yellow"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cisco.photogame.GameActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="facelift" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.cisco.photogame.HighscoreActivity"
            android:label="@string/title_activity_highscore" >
        </activity>
    </application>

</manifest>
